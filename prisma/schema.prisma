datasource db {
  provider = "mysql"
}

generator client {
  provider = "prisma-client-js"
}

model Url {
  id          Int      @id @default(autoincrement())
  shortId     String   @unique
  redirectURL String
  visits      Visit[]
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model Visit {
  id        Int      @id @default(autoincrement())
  url       Url      @relation(fields: [urlId], references: [id])
  urlId     Int
  timestamp BigInt
}
